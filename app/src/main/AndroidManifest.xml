<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.christopher.a3cpocompatibility">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CustomersActivity"
            android:label="@string/title_activity_customers"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity
            android:name=".PropertiesActivity"
            android:label="@string/title_activity_properties"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity
            android:name=".VehiclesActivity"
            android:label="@string/title_activity_vehicles"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity
            android:name=".AddVehicleActivity"
            android:label="@string/title_activity_add_vehicle"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>

        <service android:name=".GeocodeIntentService" />

        <activity android:name=".AddPropertyActivity"></activity>
    </application>

</manifest>